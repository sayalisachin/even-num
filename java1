//Create an array of 100 places storing numbers from 1 to 100, print out only the even numbers.


package hello_world;

public class example {

	public static void main(String[] args) {
		int value= 100;
		 
		int values [];
		values = new int [100];
		for (int i=0; i< values.length; i++) {
		
			values[i]= i+1;
			
					
		}
		int n=2;
		for (int j=0; j< values.length; j+=n) {
			System.out.println(values[j+1]);
			
			
		}
	
	
		
		
		
	}

}
